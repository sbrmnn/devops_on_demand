<div class="work_experience-nested-fields">
  <div class="form-row mb-2">
    <div class="form-group col-12">
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control', id: nil %>
    </div>
    <div class="form-group col-12">
      <%= f.label :employer %>
      <%= f.text_field :employer, class: 'form-control', id: nil %>
    </div>
    <div class="form-group col-12">
      <%= f.label :from %>
      <%= f.text_field :from, class: 'form-control', "data-provide":'datepicker', id: nil %>
    </div>
    <div class="form-group col-12">
      <%= f.label :to %>
      <%= f.text_field :to, class: 'form-control', "data-provide":'datepicker', id: nil %>
    </div>
    <div class="form-group col-12">
      <%= f.label :achivements %>
      <%= f.text_area :achivements, class: 'form-control', id: nil %>
    </div>
  </div>
  <div class="line-separator">
    <span class="outer-line"></span>
    <span class="fa fa-briefcase" aria-hidden="true"></span>
    <span class="outer-line"></span>
  </div>
</div>